+    changed_only = getattr(args, "changed_only", True)  # Default True for fast tier
+    tool = RuffTool(repo_root, extra_args=[], changed_only=changed_only)
+
+    status, _details = tool.run()
+
+    if status == "ok":
+        return 0
+    return 1
