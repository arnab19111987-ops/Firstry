{
  "name": "firsttry-vscode-extension",
  "displayName": "FirstTry",
  "version": "0.1.0",
  "publisher": "your-id",
  "engines": { "vscode": "^1.90.0" },
  "categories": ["Other"],
  "activationEvents": ["onCommand:firsttry.hello"],
  "main": "./dist/extension.js",
  "scripts": {
    "lint": "eslint . || echo 'skip lint'",
    "typecheck": "tsc --noEmit",
