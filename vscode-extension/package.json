{
  "name": "firsttry-extension",
  "displayName": "FirstTry",
  "description": "Run FirstTry doctor and view health reports in VS Code.",
  "version": "0.0.1",
  "publisher": "firsttry",
  "engines": { "vscode": "^1.80.0" },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:firsttry.runDoctor"
  ],
  "contributes": {
    "commands": [
      {
        "command": "firsttry.runDoctor",
        "title": "FirstTry: Run Doctor"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "vsce": "^2.0.0"
  }
}
