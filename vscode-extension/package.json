{
  "name": "firsttry-vscode-extension",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "displayName": "FirstTry",
  "scripts": {
    "lint": "echo 'skip lint for vscode-extension skeleton'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "tsc -p tsconfig.json"
  },
  "contributes": {
    "commands": [{ "command": "firsttry.hello", "title": "FirstTry: Hello" }]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.105.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0"
  }
}
